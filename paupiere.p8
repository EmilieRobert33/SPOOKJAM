function _draw()
	cls(1+c)
	rectfill(32,32,96,96,c)
	paup()
end
function init_paup()
	oeil=true
	h=128
	l=128
	f=0
	c=8
end

function paup()
	if (f<=-5) return
	if (f<=64 and oeil==true) then
		f+=1
		rectfill(0,0,128,0+f,0)
		rectfill(0,128,128,128-f,0)
		if (f>64) then 
			oeil=false
		--quand on change oeil on switch 
		--la map dans la chambre
			c=9
		end
	else
		f-=1
		rectfill(0,0,128,0+f,0)
		rectfill(0,128,128,128-f,0)
	end
end
